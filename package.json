{
  "name": "vue3-notion",
  "version": "0.1.11",
  "description": "Vue 3 Unofficial Notion Renderer",
  "homepage": "https://vue3-notion.vercel.app/",
  "repository": "github:zernonia/vue3-notion",
  "main": "dist/entry.js",
  "module": "dist/entry.esm.js",
  "browser": "dist/min.js",
  "unpkg": "dist/min.js",
  "files": [
    "dist/**/*.ts",
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/*.css",
    "src/**/*.css",
    "nuxt/*.ts"
  ],
  "keywords": [
    "vue",
    "vue3",
    "nuxt",
    "nuxt3",
    "notion"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "dev:build": "vue-tsc --noEmit && vite build",
    "build": "rollup -c && tsc --emitDeclarationOnly",
    "build:types": "vue-tsc --noEmit",
    "preview": "vite preview",
    "version": " git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@nuxt/kit-edge": "^3.0.0-27449998.32cf973",
    "fragment-for-vue": "^1.0.1",
    "katex": "^0.15.1",
    "pathe": "^0.2.0",
    "prismjs": "^1.25.0",
    "vue": "^3.2.26",
    "vue-router": "4"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/katex": "^0.11.1",
    "@types/node": "^17.0.5",
    "@types/prismjs": "^1.16.6",
    "@vitejs/plugin-vue": "^2.0.0",
    "postcss": "^8.4.5",
    "rollup": "^2.62.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-vue": "^6.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  },
  "peerDependencies": {
    "fragment-for-vue": "^1.0.1",
    "katex": "^0.15.1",
    "prismjs": "^1.25.0",
    "vue": "^3.2.20"
  }
}
